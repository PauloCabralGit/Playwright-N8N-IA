{
  "name": "playwright-n8n-ia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:report": "npx playwright show-report",
    "install-deps": "npm ci && npx playwright install --with-deps",
    "start-runner": "node test-runner-server.js",
    "test-webhook": "powershell -Command \"$url = 'http://localhost:5678/webhook/github-tests'; $payload = @{ ambiente = 'preprod' } | ConvertTo-Json; Invoke-RestMethod -Uri $url -Method Post -Body $payload -ContentType 'application/json' | ConvertTo-Json\""
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.9",
    "js-yaml": "^4.1.0"
  }
}
