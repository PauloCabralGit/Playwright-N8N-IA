# N8N Environment Variables
# Copie para seu arquivo .env do N8N e preencha com valores reais

# GitHub
GITHUB_TOKEN=ghp_seu-token-github-aqui
GITHUB_WEBHOOK_SECRET=seu-secret-webhook-github

# Discord
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/SEU_ID/SEU_TOKEN

# N8N Configuration
N8N_HOST=seu-dominio.com
N8N_PORT=5678
N8N_PROTOCOL=https

# Timezone
N8N_TIMEZONE=America/Sao_Paulo

# Database (opcional)
DB_TYPE=sqlite
DB_SQLITE_FILE=database.sqlite

# Webhook
WEBHOOK_TUNNEL_URL=https://seu-dominio.com/

# Credentials
ENCRYPTION_KEY=sua-chave-criptografia-aqui

# Logs
LOG_LEVEL=info
